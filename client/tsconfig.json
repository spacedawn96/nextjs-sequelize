{
  "compilerOptions": {
    "types": ["cypress", "@testing-library/cypress"],
    "target": "esnext",
    "module": "esnext",
    "jsx": "react",
    "lib": [
      "dom",
      "es2017"
    ],
    "baseUrl": ".",
    "typeRoots": [
      "types"
    ],
    "moduleResolution": "node",
    "strict": false,
    "allowJs": true,
    "noEmit": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "noUnusedLocals": false,
    "isolatedModules": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": false,
    "resolveJsonModule": true,
    "noFallthroughCasesInSwitch": true
  },
  "exclude": [
    "dist",
    ".next",
    "out",
    "next.config.js"
  ],
  "include": [
    "next-env.d.ts",
    "generated/**/*.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "**/*.json", "cypress/plugins/index.js"
  ]
}
